<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:Student_Management_System.ViewModel"
                    xmlns:view="clr-namespace:Student_Management_System.Views">

    <DataTemplate DataType="{x:Type vm:DashboardVM}">
        <view:Dashboard />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:StudentVM}">
        <view:Students />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:AnalyticsVM}">
        <view:Analytics />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:NewStudentVm}">
        <view:NewStudent />
    </DataTemplate>

    

    <DataTemplate DataType="{x:Type vm:StudentResultsVM}">
        <view:StudentResults />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ShowResultsVM}">
        <view:ShowResults />
    </DataTemplate>

</ResourceDictionary>